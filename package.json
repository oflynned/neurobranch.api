{
  "name": "neurobranch.api",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start:core": "nx serve core",
    "build": "nx build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"**/*.ts\" --fix",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "lint:code:staged": "lint-staged",
    "lint:dependencies": "sort-package-json",
    "lint:dependencies:versions": "ncu -e 2 --target minor",
    "upgrade:dependencies": "ncu -u --target minor && npm install",
    "db:migrate:up": "ts-node ./apps/core/src/app/migrations/up.ts",
    "db:migrate:down": "ts-node ./apps/core/src/app/migrations/down.ts",
    "typeorm": "ts-node -r tsconfig-paths/register --project tsconfig.json ./node_modules/typeorm/cli.js",
    "format:code": "prettier --write \"**/*.ts\"",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.js": "eslint --cache --fix",
    "*.ts": "prettier --write"
  },
  "dependencies": {
    "@aginix/nestjs-firebase-admin": "^2.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.23.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "ioredis": "^4.27.1",
    "nestjs-redis": "^1.3.3",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "12.0.8",
    "@nrwl/eslint-plugin-nx": "12.0.8",
    "@nrwl/jest": "12.0.8",
    "@nrwl/linter": "12.0.8",
    "@nrwl/nest": "12.0.8",
    "@nrwl/node": "12.0.8",
    "@nrwl/tao": "12.0.8",
    "@nrwl/workspace": "12.0.8",
    "@types/faker": "^5.5.3",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "faker": "^5.5.3",
    "husky": "^4.3.8",
    "jest": "26.2.2",
    "jest-mock-extended": "^1.0.14",
    "lint-staged": "^10.5.4",
    "npm-check-updates": "^11.5.7",
    "prettier": "2.2.1",
    "sort-package-json": "^1.49.0",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.4"
  }
}
