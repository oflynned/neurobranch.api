{
  "dependencies": {
    "@aginix/nestjs-firebase-admin": "^2.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/platform-express": "^7.0.0",
    "@prisma/client": "^2.23.0",
    "apollo-server-express": "^2.23.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.22.1",
    "graphql": "^15.5.0",
    "ioredis": "^4.27.1",
    "nestjs-redis": "^1.3.3",
    "pg": "^8.6.0",
    "prisma": "^2.23.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "12.0.8",
    "@nrwl/eslint-plugin-nx": "12.0.8",
    "@nrwl/jest": "12.0.8",
    "@nrwl/linter": "12.0.8",
    "@nrwl/nest": "12.0.8",
    "@nrwl/node": "12.0.8",
    "@nrwl/tao": "12.0.8",
    "@nrwl/workspace": "12.0.8",
    "@types/faker": "^5.5.3",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "faker": "^5.5.3",
    "graphql-tools": "^7.0.5",
    "husky": "^4.3.8",
    "jest": "26.2.2",
    "jest-mock-extended": "^1.0.14",
    "lint-staged": "^10.5.4",
    "npm-check-updates": "^11.5.7",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "^2.1.0",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:dependencies && npm run lint:code:staged",
      "pre-push": "npm test"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts": "prettier --write",
    "*.{js,ts}": "eslint --cache --fix"
  },
  "name": "neurobranch.api",
  "private": true,
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "nx build",
    "build:core": "nx build core --production --no-cache",
    "db:migrate:down": "node -r ts-node/register apps/core/src/migrations/down.ts",
    "db:migrate:up": "node -r ts-node/register apps/core/src/migrations/up.ts",
    "dep-graph": "nx dep-graph",
    "e2e": "nx e2e",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "nx format:check",
    "help": "nx help",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:code:staged": "lint-staged",
    "lint:dependencies": "node ./scripts/sort-package-json.js",
    "lint:dependencies:versions": "ncu -e 2 --target minor",
    "migrate:reset": "prisma migrate reset",
    "migrate:sync": "prisma migrate dev",
    "migrate:up": "prisma migrate deploy",
    "nx": "nx",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "start": "npm run start:core",
    "start:core": "npm run types:merge && MODE=production nx serve core",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "types:gen": "ts-node ./libs/graphql/src/generate-typings.ts",
    "types:merge": "ts-node ./apps/core/src/merge-definitions.ts",
    "update": "nx migrate latest",
    "upgrade:dependencies": "ncu -u --target minor && npm install",
    "workspace-generator": "nx workspace-generator"
  },
  "version": "0.2.2"
}
